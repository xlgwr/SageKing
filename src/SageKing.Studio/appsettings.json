{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "IceRPCServer": {
    "Name": "server1", //不能为空，唯一
    "ServerAddress": "icerpc://[::0]:4062",
    "IsQuic": true,
    "IsTcpTLS": true,
    "ServerCertificateFileName": "certs/server.p12"
  },

  "IceRPCClients": [
    {
      "ServerName": "server1", //服务器不能为空，唯一
      "ServerAddress": "icerpc://localhost:4062",
      "IsQuic": true,
      "IsTcpTLS": true,
      "ServerCertificateFileName": "certs/cacert.der",
      "Timeout": 10
    }
  ]
}
