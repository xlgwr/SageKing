{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "IceRPCServer": {
    "ServerName": "server1", //不能为空，唯一
    "ServerAddress": "icerpc://[::0]:4062",
    "IsQuic": false,
    "IsTcpTLS": true,
    "UseCompressor": false,
    "CertificateFileName": "certs/server.p12"
  },

  "IceRPCClients": [
    {
      "ClientId": "client1", // 客户端id
      "ClientType": 1, // 客户端类型
      "Timeout": 10,

      "ServerName": "server1", // 服务器不能为空，唯一
      "ServerAddress": "icerpc://localhost:4062",
      "IsQuic": false,
      "IsTcpTLS": true,
      "UseCompressor": false,
      "CertificateFileName": "certs/cacert.der"
    }
  ]
}
